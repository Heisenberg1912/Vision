  "microcopy": {
    "terrainSoil": "Means: base construction risk. Inputs: terrain+soil+image cues. Assumption: no borelog provided.",
    "climateDensity": "Means: demand and execution pressure. Inputs: climate+density signals. Assumption: seasonality is typical.",
    "masterPlanZone": "Means: allowed use and value ceiling. Inputs: master-plan zone+project type. Assumption: current zoning remains active.",
    "politicalLeaning": "Means: approvals and demand bias. Inputs: policy posture+policy focus. Assumption: policy stance stays near-term stable.",
    "comparableActivity": "Means: market anchor depth. Inputs: comp count+activity level. Assumption: comps are locally relevant.",
    "typology": "Means: asset class fit. Inputs: category matrix typology. Assumption: image classification is correct.",
    "stage": "Means: current execution phase. Inputs: progress+stage inference. Assumption: photo reflects current site status.",
    "propertyVal": "Means: total asset value band. Inputs: comps, geo, zoning, age, growth. Assumption: no hidden legal defects.",
    "landVal": "Means: land-only value band. Inputs: land share, zoning, location, land growth. Assumption: title and access are clean.",
    "projectVal": "Means: as-is project value band. Inputs: stage completion + land + market factors. Assumption: build quality is standard.",
    "policyFocus": "Means: planning intent driver. Inputs: policy focus + zoning context. Assumption: no abrupt policy reversal.",
    "cityGrowth": "Means: top-down momentum. Inputs: city 5Y growth estimate. Assumption: trend does not sharply reverse.",
    "propertyGrowth": "Means: built-asset trend. Inputs: local property growth estimate. Assumption: sample market is representative.",
    "landGrowth": "Means: land demand trend. Inputs: local land growth estimate. Assumption: supply additions remain normal.",
    "propertyAge": "Means: depreciation pressure. Inputs: inferred age and condition. Assumption: maintenance quality is average.",
    "resaleValue": "Means: exit strength index. Inputs: age, liquidity, demand fit. Assumption: no distress sale required.",
    "roiEst": "Means: return potential estimate. Inputs: growth, liquidity, policy, comparables. Assumption: entry cost is market-aligned.",
    "valuationPanel": "Means: valuation is range-based. Inputs: A-F weighted model + confidence spread. Assumption: no off-book constraints.",
    "roiMeter": "Means: normalized ROI signal. Inputs: inferred ROI estimate. Assumption: hold period and financing are typical.",
    "marketPerformance": "Means: momentum diagnostics. Inputs: growth, age, resale, ROI. Assumption: macro conditions stay neutral."
  },
